    <style>
        @import url('https://fonts.googleapis.com/css2?family=VT323&display=swap');
        
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            background: #000;
            color: #fff;
            font-family: 'VT323', monospace;
            overflow-x: hidden;
            min-height: 100vh;
            cursor: crosshair;
        }

        .glitch-container {
            position: relative;
            min-height: 100vh;
            background: linear-gradient(87deg, #000, #702d3b, #000);
            animation: bgPulse 8s ease infinite;
            padding: 20px;
        }

        .profile-section {
            max-width: 800px;
            margin: 40px auto;
            background: rgba(0, 0, 0, 0.8);
            border: 1px solid #a1002b;
            padding: 20px;
            position: relative;
            z-index: 1;
        }

        .profile-header {
            display: flex;
            align-items: center;
            gap: 20px;
            margin-bottom: 30px;
        }

        .profile-pic {
            width: 150px;
            height: 150px;
            border: 2px solid #702d3b;
            position: relative;
            overflow: hidden;
        }

        .profile-pic img {
            width: 100%;
            height: 100%;
            object-fit: cover;
            filter: brightness(0.8) contrast(1.2);
        }

        .profile-pic::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: linear-gradient(45deg, transparent, #702d3b);
            animation: glowPulse 4s infinite;
        }

        .nameplate {
            font-size: 2.5em;
            color: #a1002b;
            text-shadow: 2px 2px ##702d3b;
            animation: glitchText 3s infinite;
        }

        .links-section {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));
            gap: 10px;
            margin: 20px 0;
        }

        .link-button {
            background: rgba(0, 255, 255, 0.1);
            border: 1px solid #a1002b;
            padding: 10px;
            color: #fff;
            text-decoration: none;
            text-align: center;
            transition: all 0.3s;
        }

        .link-button:hover {
            background: rgba(255, 0, 255, 0.2);
            transform: scale(1.05);
            text-shadow: 0 0 5px #a1002b;
        }

        .preferences {
            display: flex;
            gap: 20px;
            margin: 20px 0;
        }

        .likes, .dislikes {
            flex: 1;
            background: rgba(0, 0, 0, 0.5);
            padding: 15px;
            border: 1px solid #a1002b;
        }

        .spotify-embed {
            margin: 20px 0;
            border: 1px solid #a1002b;
            padding: 10px;
        }

        .button-collection {
            display: flex;
            flex-wrap: wrap;
            gap: 5px;
            margin: 20px 0;
            padding: 10px;
            background: #33000e;
            border: 1px dashed #0ff;
        }

        .button-collection img {
            width: 88px;
            height: 31px;
            object-fit: contain;
        }

        .modal {
            display: none;
            position: fixed;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            background: #33000e;
            border: 2px solid #ff00ff;
            padding: 20px;
            z-index: 1000;
            max-width: 80%;
            max-height: 80vh;
            overflow-y: auto;
        }

        .modal.active {
            display: block;
            animation: modalGlitch 0.3s;
        }

        .modal-close {
            position: absolute;
            top: 10px;
            right: 10px;
            color: #33000e;
            cursor: pointer;
            font-size: 1.5em;
        }

        @keyframes modalGlitch {
            0% { transform: translate(-50%, -50%) skew(-10deg); }
            50% { transform: translate(-50%, -50%) skew(10deg); }
            100% { transform: translate(-50%, -50%) skew(0deg); }
        }

        @keyframes glitchText {
            0% { transform: skew(0deg); }
            20% { transform: skew(-3deg); color: #0ff; }
            40% { transform: skew(3deg); color: #ff00ff; }
            60% { transform: skew(0deg); }
            80% { transform: skew(2deg); color: #0ff; }
            100% { transform: skew(0deg); }
        }

        @keyframes glowPulse {
            0% { opacity: 0.3; }
            50% { opacity: 0.7; }
            100% { opacity: 0.3; }
        }
      
        .floating-entities {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            pointer-events: none;
            z-index: 0;
        }

        .floating-gif {
            position: absolute;
            opacity: 0.3;
            filter: brightness(0.7) contrast(1.2) hue-rotate(90deg);
            animation: float-random 20s linear infinite;
        }

        @keyframes float-random {
            0% { transform: translate(0, 0) rotate(0deg); }
            50% { transform: translate(100px, -100px) rotate(180deg); }
            100% { transform: translate(0, 0) rotate(360deg); }
        }

        .hud {
            position: fixed;
            top: 200px;
            left: 20px;
            background: rgba(128, 92, 98);
            border: 1px solid #805c62;
            padding: 10px;
            font-size: 0.8em;
            z-index: 1000;
        }

        .hud-data {
            color: #805c62;
            text-shadow: 0 0 5px #805c62;
        }

        .marquee-container {
            overflow: hidden;
            background: rgba(128, 92, 98);
            border: 1px dashed #805c62;
        }

        .marquee {
            display: flex;
            animation: scroll 20s linear infinite;
        }

        @keyframes scroll {
            0% { transform: translateX(0); }
            100% { transform: translateX(-50%); }
        }

        .secret-menu {
            position: fixed;
            right: -300px;
            top: 50%;
            transform: translateY(-50%);
            width: 300px;
            background: rgba(128, 92, 98);
            border-left: 2px solid #ff00ff;
            padding: 20px;
            transition: right 0.3s;
            z-index: 1000;
        }

        .secret-menu.active {
            right: 0;
        }

        .secret-trigger {
            position: fixed;
            right: 0;
            top: 50%;
            transform: translateY(-50%);
            width: 20px;
            height: 100px;
            background: #ff00ff;
            cursor: pointer;
            z-index: 999;
        }

        .tab-container {
            margin-top: 20px;
        }

        .tab-buttons {
            display: flex;
            gap: 5px;
            margin-bottom: 10px;
        }

        .tab-button {
            background: none;
            border: 1px solid #0ff;
            color: #0ff;
            padding: 5px 10px;
            cursor: pointer;
        }

        .tab-button.active {
            background: rgba(128, 92, 98);
        }

        .tab-content {
            display: none;
            padding: 10px;
            border: 1px solid #0ff;
        }

        .tab-content.active {
            display: block;
        }

        /* Particle system */
        #particles-js {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            pointer-events: none;
            z-index: 0;
        }
    </style>
</head>
